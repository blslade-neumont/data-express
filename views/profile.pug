extends layout

block content
    h1 Profile
    p Username: #{req.user.username}
    p Email: #{req.user.email}
    p Age: #{req.user.age}
    p Ugliest animal: #{utils.q1Response(req.user.q1)}
    p Most important coding aspect: #{utils.q2Response(req.user.q2)}
    p 20th President: #{utils.q3Response(req.user.q3)}
    a(href='/edit-profile') Edit Profile
